{
  "name": "rivage",
  "productName": "Rivage",
  "version": "0.0.1",
  "description": "A tag file explorer",
  "license": "MIT",
  "repository": "https://github.com/frinyvonnick/rivage",
  "homepage": "https://github.com/frinyvonnick/rivage",
  "bugs": "frin.yvonnick@gmail.com",
  "engines": {},
  "contributors": [],
  "author": {
    "name": "FRIN Yvonnick",
    "email": "frin.yvonnick@gmail.com",
    "url": "https://github.com/frinyvonnick"
  },
  "_moduleAliases": {
    "package.json": "./package.json",
    "src": "./src",
    "state": "./src/state",
    "assets": "./src/assets",
    "windows": "./src/windows",
    "main-process": "./src/main-process",
    "utils": "./src/utils",
    "renderer-process": "./src/renderer-process"
  },
  "options": {
    "initialPath": "/Users/PC/Documents/Perso/"
  },
  "build": {
    "publish": {
      "GithubOptions": {}
    }
  },
  "scripts": {
    "start": "electron --harmony src",
    "predev": "yarn build-components",
    "dev": "cross-env NODE_ENV=development yarn watch-app-files",
    "lint": "eslint src",
    "pack": "build --dir -mwl --x64",
    "dist": "build -mwl --x64",
    "watch-app-files": "nodemon --config nodemon.json",
    "watch-components": "nodemon --config nodemon-svelte.json",
    "build-components": "svelte compile -f umd -i ./src/components/ -o ./src/components/"
  },
  "keywords": [
    "tags",
    "electron"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "chokidar": "^1.7.0",
    "classnames": "^2.2.5",
    "electron-debug": "^1.2.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-json-storage": "^3.0.6",
    "electron-reload": "https://github.com/frinyvonnick/electron-reload",
    "electron-updater": "^2.7.1",
    "electron-window": "^0.8.1",
    "eslint": "^4.2.0",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.1.0",
    "is-electron-renderer": "^2.0.1",
    "js-autocomplete": "^1.0.4",
    "lodash": "^4.17.4",
    "module-alias": "^2.0.0",
    "redux": "^3.7.2",
    "redux-devtools-instrument": "^1.8.2",
    "redux-electron-store": "^0.4.1",
    "remote-redux-devtools": "^0.5.12",
    "shallow-equal": "^1.0.0",
    "svelte-cli": "^1.3.7"
  },
  "devDependencies": {
    "cross-env": "^5.0.4",
    "devtron": "^1.1.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.0",
    "nodemon": "^1.11.0",
    "redux-logger": "^3.0.6"
  }
}
